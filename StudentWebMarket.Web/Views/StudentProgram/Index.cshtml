@model IEnumerable<StudentWebMarket.Models.Models.StudentProgram>

@{
    ViewBag.Title = "Index";
}

<h2>Student Market Management</h2>

<a href="@Url.Action("Create","StudentProgram")" style="margin-bottom:10px;" class="btn btn-primary"><i class="fa fa-plus"></i> Create a student program</a>

<br />
<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Student Program")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>                
                    <i class="fa fa-graduation-cap"></i> &nbsp;
                    @Html.DisplayFor(modelItem => item.Name)
                
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.StudentProgramId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.StudentProgramId })
            </td>
        </tr>
    }
</table>


<a class='btn btn-default' href="@Url.Action("Index", "Management")" style='width:80px'>
    <i class='glyphicon glyphicon-chevron-left'></i> Back
</a>
